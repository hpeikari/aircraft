{
  "name": "Airline",
  "description": "Henry P. - Airline App",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start:client": "cross-env NODE_OPTIONS=--max-old-space-size=4096 webpack serve --progress --config config/webpack.dev.config.js --history-api-fallback",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll",
    "test:all": "jest --coverage --no-cache --coverageDirectory output",
    "lint:js": "eslint src/",
    "lint:css": "stylelint --config config/stylelint.config.json **/*.scss",
    "test": "npm-run-all test:all lint:*",
    "start": "npm run-script start:client",
    "format": "prettier --write \"src/**/*.+(js|jsx|json|scss|md)\"",
    "format:lint": "eslint --fix \"src/**/*.+(js|jsx|json|scss|md)\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm-run-all lint:*"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,md,html,scss,css}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-proposal-optional-chaining": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.12.13",
    "@babel/runtime": "^7.13.10",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.2.2",
    "circular-dependency-plugin": "^5.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.3",
    "enzyme": "^3.11.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^2.5.1",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^4.3.8",
    "jest-cli": "^26.6.3",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.3.9",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "open-sans-fontface": "^1.4.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.2.8",
    "postcss-loader": "^5.2.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "redux-mock-store": "^1.5.4",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "stylelint": "^8.1.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-prettier": "^1.1.2",
    "svg-react-loader": "^0.4.6",
    "terser-webpack-plugin": "^5.1.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.26.3",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "classlist-polyfill": "^1.2.0",
    "core-js": "^3.9.1",
    "focus-visible": "^5.2.0",
    "history": "^4.10.1",
    "i18next": "^19.0.1",
    "i18next-browser-languagedetector": "^4.0.1",
    "i18next-xhr-backend": "^3.2.2",
    "prop-types": "^15.7.2",
    "qs": "^6.10.0",
    "ramda": "^0.27.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.2.2",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-widgets": "^5.0.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.7",
    "reselect": "^4.0.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(svg)$": "<rootDir>/__mocks__/assetMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/cssModuleMock.js"
    },
    "setupFiles": [
      "<rootDir>/test/rafPolyfill.js",
      "<rootDir>/test/enzymeSetup.js"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!tools/**",
      "!build/**",
      "!output/**",
      "!config/**",
      "!src/index.js",
      "!src/routes/index.js",
      "!src/redux/actions/**",
      "!src/constants/**",
      "!src/views/**",
      "!src/i18n/*.js",
      "!src/mock/*.js",
      "!src/config.js",
      "!build/**",
      "!test/**"
    ]
  }
}
